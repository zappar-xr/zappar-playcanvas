{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-undef" : "off",
        "func-names" : "off",
        "no-console" : "off",
        "rules": {
            "max-len": ["error", { "code": 120 }]
        }
    }
}
